  },
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@types/jest": "^26.0.9",
    "@types/react": "^16.9.46",
    "@types/react-native": "^0.63.6",
    "@types/react-test-renderer": "^16.9.3",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-react": "^7.20.5",
    "husky": ">=4",
    "jest": "^26.3.0",
